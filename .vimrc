set nocompatible

" Syntax highlighting
syntax on

" -- General Settings
set mouse=a
set number
set nowrap
set cmdheight=2
set wildmenu
set showcmd
set showmode
set ttyfast
set encoding=utf-8
set hlsearch
set incsearch
set ignorecase
set smartcase
set expandtab
set shiftwidth=2
set tabstop=2
set autoindent
 
